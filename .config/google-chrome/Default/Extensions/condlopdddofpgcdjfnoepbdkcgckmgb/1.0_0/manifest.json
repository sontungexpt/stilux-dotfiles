{
   "background": {
      "persistent": false,
      "scripts": [ "js/common.js", "js/background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "description": "Right Click to translate selected text using Google Translate",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Translator",
   "options_ui": {
      "chrome_style": true,
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "tabs", "activeTab", "contextMenus", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
