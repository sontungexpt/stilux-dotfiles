{
   "background": {
      "scripts": [ "background/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/128.png",
         "16": "icons/16.png",
         "19": "icons/19.png",
         "32": "icons/32.png",
         "38": "icons/38.png",
         "48": "icons/48.png",
         "512": "icons/512.png",
         "64": "icons/64.png"
      },
      "default_popup": "popup/index.html"
   },
   "commands": {
      "_execute_browser_action": {
         "description": "__MSG_openPopupDescription__",
         "suggested_key": {
            "default": "Ctrl+Shift+Space"
         }
      },
      "translatePage": {
         "description": "__MSG_translatePageMenu__"
      },
      "translateSelectedText": {
         "description": "__MSG_translateTextMenu__",
         "suggested_key": {
            "default": "Ctrl+Space"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content/content.css" ],
      "js": [ "content/content.js", "content/jquery-3.4.1.min.js" ],
      "match_about_blank": true,
      "matches": [ "http://*/*", "https://*/*", "\u003Call_urls>" ]
   } ],
   "default_locale": "en",
   "description": "Traduction One Click -- Simple translation with just one click",
   "icons": {
      "128": "icons/128.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "512": "icons/512.png",
      "64": "icons/64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Traduction One Click",
   "options_ui": {
      "open_in_tab": true,
      "page": "options/index.html"
   },
   "permissions": [ "\u003Call_urls>", "storage", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1",
   "web_accessible_resources": [ "icons/512.png" ]
}
