{
   "action": {
      "default_icon": "images/logo.png"
   },
   "content_scripts": [ {
      "js": [ "main.js" ],
      "matches": [ "https://drive.google.com/*" ]
   } ],
   "description": "Dark mode for Google Drive!",
   "icons": {
      "128": "images/logo.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Google Drive Dark Mode",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.3",
   "web_accessible_resources": [ {
      "matches": [ "https://drive.google.com/*", "https://docs.google.com/*" ],
      "resources": [ "main.css", "permanent.css", "images/moon.png", "images/sun.png", "images/paypal.png", "images/logo.png" ]
   } ]
}
